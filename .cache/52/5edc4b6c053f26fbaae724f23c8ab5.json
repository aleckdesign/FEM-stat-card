lue (card background): hsl(244, 38%, 16%)\r\n// - Soft violet (accent): hsl(277, 64%, 61%)\r\n\r\n// ### Neutral\r\n\r\n// - White (main heading, stats): hsl(0, 0%, 100%)\r\n// - Slightly transparent white (main paragraph): hsla(0, 0%, 100%, 0.75)\r\n// - Slightly transparent white (stat headings): hsla(0, 0%, 100%, 0.6)\r\n\r\n$color-primary: hsl(244, 38%, 16%);\r\n$color-primary-dark: hsl(233, 47%, 7%);\r\n$color-primary-accent: hsl(277, 64%, 61%);\r\n\r\n$color-white: hsl(0, 0%, 100%);\r\n$color-white-2: hsla(0, 0%, 100%, 0.75);\r\n$color-white-3: hsla(0, 0%, 100%, 0.6);\r\n","scss\\_statsPreview.scss":"@use 'abstracts' as *;\r\n\r\n.card {\r\n  display: flex;\r\n  background-color: $color-primary;\r\n  max-width: 1440px;\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n  border-radius: 0.5rem;\r\n  overflow: hidden;\r\n\r\n  @media (max-width: 800px) {\r\n    max-width: 375px;\r\n    min-width: 280px;\r\n    height: 550px;\r\n    flex-direction: column;\r\n  }\r\n\r\n  &__left {\r\n    display: flex;\r\n    flex-direction: column;\r\n    grid-template-columns: repeat(3, 1fr);\r\n    width: 50%;\r\n    padding: 4rem;\r\n\r\n    @media (max-width: 800px) {\r\n      order: 2;\r\n      width: 100%;\r\n      height: 50%;\r\n      align-items: center;\r\n      text-align: center;\r\n      margin-bottom: 2rem;\r\n      padding: 3.5rem;\r\n    }\r\n  }\r\n\r\n  &__heading {\r\n    font-size: 2rem;\r\n    color: $color-white;\r\n    margin-bottom: 1.5rem;\r\n\r\n    & span {\r\n      color: $color-primary-accent;\r\n    }\r\n  }\r\n\r\n  &__text {\r\n    color: $color-white-3;\r\n    font-size: 1rem;\r\n    line-height: 1.5rem;\r\n    margin-bottom: 4rem;\r\n\r\n    @media (max-width: 800px) {\r\n      margin-bottom: 3rem;\r\n    }\r\n  }\r\n\r\n  &__stats {\r\n    display: grid;\r\n    grid-template-columns: repeat(3, 1fr);\r\n    font-size: 1.4rem;\r\n\r\n    @media (max-width: 800px) {\r\n      grid-template-columns: max-content;\r\n      row-gap: 2rem;\r\n    }\r\n  }\r\n\r\n  &__right-overlay {\r\n    width: 100%;\r\n    height: 100%;\r\n    background-color: $color-primary-accent;\r\n    opacity: 1;\r\n    mix-blend-mode: hard-light;\r\n  }\r\n\r\n  &__right {\r\n    width: 50%;\r\n    background-image: url(../images/image-header-desktop.jpg);\r\n    background-size: cover;\r\n    background-position: top;\r\n    filter: brightness(0.8);\r\n\r\n    @media (max-width: 800px) {\r\n      order: 1;\r\n      width: 100%;\r\n      height: 35%;\r\n      background-image: url(../images/image-header-mobile.jpg);\r\n    }\r\n  }\r\n}\r\n\r\n.heading-2 {\r\n  color: $color-white;\r\n  margin-bottom: 5px;\r\n}\r\n.heading-3 {\r\n  font-size: 0.6rem;\r\n  letter-spacing: 1.2px;\r\n  text-transform: uppercase;\r\n  color: $color-white-3;\r\n  font-weight: 300;\r\n}\r\n\r\n.attribution {\r\n  font-size: 11px;\r\n  text-align: center;\r\n}\r\n.attribution a {\r\n  color: hsl(228, 45%, 44%);\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"624b24e038651cc925687103eb1cb6a5","cacheData":{"env":{}}}